
public class Symbol {

	Equation eq;
	public Symbol(Equation eq) {
		this.eq = eq;
	}
	
	public boolean isNonTerminal() {
		if(value == 0) {
			return true;
		} else {
			return false;
		}
	}
	
	public boolean isTerminal() {
		if(value == 1) {
			return true;
		} else {
			return false;
		}
	}
	
	public boolean Operand() {
		if(value == 2) {
			return true;
		} else {
			return false;
		}
	}
}
